generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model audience_growth_rate {
  date                                             DateTime? @db.Timestamp(6)
  audience_growth_rate_daily__all_countries        Float?
  audience_growth_rate_daily__egypt                Float?
  audience_growth_rate_daily__saudi_arabia         Float?
  audience_growth_rate_daily__united_states        Float?
  audience_growth_rate_daily__united_arab_emirates Float?
  id                                               BigInt    @id @default(autoincrement())
}

model average_rating {
  date                                             DateTime? @db.Timestamp(6)
  average_rating_per_interval_daily__all_countries String?
  average_rating_per_interval_daily__egypt         String?
  id                                               BigInt    @id @default(autoincrement())
}

model daily_active_users {
  date                      DateTime? @db.Timestamp(6)
  dau__all_countries        Int?
  dau__egypt                Int?
  dau__saudi_arabia         Int?
  dau__united_states        Int?
  dau__united_arab_emirates Int?
  id                        BigInt    @id @default(autoincrement())
}

model device_acquisition {
  date                                     DateTime? @db.Timestamp(6)
  device_acquisition__all_countries        Int?
  device_acquisition__united_arab_emirates Int?
  id                                       BigInt    @id @default(autoincrement())
}

model facebook_demographics {
  Report__Date           DateTime? @map("Report: Date") @db.Timestamp(6)
  Audience__Country      String?   @map("Audience: Country")
  Engagement__Page_likes Int?      @map("Engagement: Page likes")
  id                     BigInt    @id @default(autoincrement())
}

model facebook_insights {
  Report__Date                  DateTime? @map("Report: Date") @db.Timestamp(6)
  Engagement__Page_views        Int?      @map("Engagement: Page views")
  Engagement__New_followers     Int?      @map("Engagement: New followers")
  Engagement__New_Page_likes    Int?      @map("Engagement: New Page likes")
  Engagement__New_Page_unlikes  Int?      @map("Engagement: New Page unlikes")
  Engagement__Posts_engagements Int?      @map("Engagement: Posts engagements")
  Performance__Impressions      Int?      @map("Performance: Impressions")
  Performance__Reach            Int?      @map("Performance: Reach")
  id                            BigInt    @id @default(autoincrement())
}

model first_opens {
  date                                                 DateTime? @db.Timestamp(6)
  first_opens_per_interval_daily__all_countries        Int?
  first_opens_per_interval_daily__egypt                Int?
  first_opens_per_interval_daily__saudi_arabia         Int?
  first_opens_per_interval_daily__united_states        Int?
  first_opens_per_interval_daily__united_arab_emirates Int?
  id                                                   BigInt    @id @default(autoincrement())
}

model google_play_ratings {
  date                                     DateTime? @db.Timestamp(6)
  google_play_rating__all_countries        Float?
  google_play_rating__united_arab_emirates Float?
  google_play_rating__antigua_and_barbuda  Float?
  google_play_rating__albania              Float?
  google_play_rating__armenia              Float?
  id                                       BigInt    @id @default(autoincrement())
}

model instagram_ageandgender {
  Audience__Age         String? @map("Audience: Age")
  Audience__Gender      String? @map("Audience: Gender")
  Engagement__Followers Int?    @map("Engagement: Followers")
  id                    BigInt  @id @default(autoincrement())
}

model instagram_demographics {
  Audience__Country     String? @map("Audience: Country")
  Engagement__Followers Int?    @map("Engagement: Followers")
  id                    BigInt  @id @default(autoincrement())
}

model instagram_followers {
  Report__Date              DateTime? @map("Report: Date") @db.Timestamp(6)
  Performance__Reach        Int?      @map("Performance: Reach")
  Engagement__New_followers Int?      @map("Engagement: New followers")
  id                        BigInt    @id @default(autoincrement())
}

model instagram_insights {
  Report__Start_date                DateTime? @map("Report: Start date") @db.Timestamp(6)
  Report__End_date                  DateTime? @map("Report: End date") @db.Timestamp(6)
  Performance__Reach                Int?      @map("Performance: Reach")
  Engagement__Views                 Int?      @map("Engagement: Views")
  Engagement__Follows_and_unfollows String?   @map("Engagement: Follows and unfollows")
  Performance__Visits__views_       Int?      @map("Performance: Visits (views)")
  Performance__Engagements          Int?      @map("Performance: Engagements")
  Engagement__Accounts_engaged      Int?      @map("Engagement: Accounts engaged")
  Clicks__Website_clicks            Int?      @map("Clicks: Website clicks")
  Clicks__Profile_links_clicks      Int?      @map("Clicks: Profile links clicks")
  id                                BigInt    @id @default(autoincrement())
}

model installed_audience {
  date                                     DateTime? @db.Timestamp(6)
  installed_audience__all_countries        Int?
  installed_audience__united_arab_emirates Int?
  installed_audience__albania              Int?
  id                                       BigInt    @id @default(autoincrement())
}

model installed_base {
  date                                     DateTime? @db.Timestamp(6)
  installed_audience__all_countries        Int?
  installed_audience__united_arab_emirates Int?
  installed_audience__albania              Int?
  id                                       BigInt    @id @default(autoincrement())
}

model monthly_active_users {
  date                      DateTime? @db.Timestamp(6)
  mau__all_countries        Int?
  mau__egypt                Int?
  mau__saudi_arabia         Int?
  mau__united_states        Int?
  mau__united_arab_emirates Int?
  id                        BigInt    @id @default(autoincrement())
}

model tiktok_insights {
  Account_ID   String? @map("Account ID")
  Account_name String? @map("Account name")
  Currency     String?
  Cost_1       Float?
  CPC          Float?
  CPM          Float?
  Impression   Int?
  Clicks       Int?
  id           BigInt  @id @default(autoincrement())
}

model total_no_of_installation {
  date                                     DateTime? @db.Timestamp(6)
  installed_audience__all_countries        Int?
  installed_audience__egypt                Int?
  installed_audience__saudi_arabia         Int?
  installed_audience__united_states        Int?
  installed_audience__united_arab_emirates Int?
  id                                       BigInt    @id @default(autoincrement())
}

model user_loss_rate {
  date                                       DateTime? @db.Timestamp(6)
  user_loss_rate_daily__all_countries        Float?
  user_loss_rate_daily__egypt                Float?
  user_loss_rate_daily__saudi_arabia         String?
  user_loss_rate_daily__united_states        String?
  user_loss_rate_daily__united_arab_emirates String?
  id                                         BigInt    @id @default(autoincrement())
}
